                                <swrl:argument1 rdf:resource="http://www.semanticweb.org/donald/ontologies/2024/9/estoppoc#m"/>
                            </rdf:Description>
                        </rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                <rdf:first>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                        <swrl:propertyPredicate rdf:resource="http://www.semanticweb.org/donald/ontologies/2024/9/estoppoc#spans"/>
                                        <swrl:argument1 rdf:resource="http://www.semanticweb.org/donald/ontologies/2024/9/estoppoc#e"/>
                                        <swrl:argument2 rdf:resource="http://www.semanticweb.org/donald/ontologies/2024/9/estoppoc#m"/>
                                    </rdf:Description>
                                </rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                        <rdf:first>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom"/>
                                                <swrl:propertyPredicate rdf:resource="http://www.semanticweb.org/donald/ontologies/2024/9/estoppoc#isEngaged"/>
                                                <swrl:argument1 rdf:resource="http://www.semanticweb.org/donald/ontologies/2024/9/estoppoc#e"/>
                                                <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</swrl:argument2>
                                            </rdf:Description>
                                        </rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
